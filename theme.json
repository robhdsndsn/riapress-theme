{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "customDuotone": false,
      "customGradient": false,
      "link": true,
      "defaultGradients": true,
      "defaultPalette": false,
      "palette": [
        {
          "slug": "white",
          "name": "White",
          "color": "var(--wp--custom--colors--white)"
        },
        {
          "slug": "neutral",
          "name": "Neutral",
          "color": "var(--wp--custom--colors--primary-50)"
        },
        {
          "slug": "dark",
          "name": "Dark",
          "color": "var(--wp--custom--colors--dark)"
        },
        {
          "slug": "navy",
          "name": "Navy",
          "color": "var(--wp--custom--colors--navy)"
        },
        {
          "slug": "navy-300",
          "name": "Navy 300",
          "color": "var(--wp--custom--colors--primary-300)"
        },
        {
          "slug": "green",
          "name": "Green",
          "color": "var(--wp--custom--colors--green)"
        }
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1400px"
    },
    "useRootPaddingAwareAlignments": true,
    "typography": {
      "customFontSize": false,
      "lineHeight": false,
      "dropCap": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "textDecoration": false,
      "textTransform": false,
      "fluid": true,
      "defaultFontSizes": false,
      "fontSizes": [
        {
          "name": "XX-Small",
          "size": "0.625rem",
          "slug": "xx-small",
          "fluid": {
            "min": "0.625rem",
            "max": "0.75rem"
          }
        },
        {
          "name": "X-Small",
          "size": "0.75rem",
          "slug": "x-small",
          "fluid": {
            "min": "0.75rem",
            "max": "0.875rem"
          }
        },
        {
          "name": "Small",
          "size": "0.875rem",
          "slug": "small",
          "fluid": {
            "min": "0.875rem",
            "max": "1rem"
          }
        },
        {
          "name": "Medium",
          "size": "1rem",
          "slug": "medium",
          "fluid": {
            "min": "1rem",
            "max": "1.25rem"
          }
        },
        {
          "name": "Large",
          "size": "1.25rem",
          "slug": "large",
          "fluid": {
            "min": "1.25rem",
            "max": "1.5rem"
          }
        },
        {
          "name": "X-Large",
          "size": "1.5rem",
          "slug": "x-large",
          "fluid": {
            "min": "1.5rem",
            "max": "2rem"
          }
        },
        {
          "name": "XX-Large",
          "slug": "xx-large",
          "size": "2.75rem",
          "fluid": {
            "min": "2.75rem",
            "max": "5rem"
          }
        },
        {
          "name": "Heading 1",
          "slug": "heading-1",
          "size": "2.5rem",
          "fluid": {
            "min": "2.5rem",
            "max": "3.5rem"
          }
        },
        {
          "name": "Heading 2",
          "slug": "heading-2",
          "size": "2rem",
          "fluid": {
            "min": "2rem",
            "max": "3rem"
          }
        },
        {
          "name": "Heading 3",
          "slug": "heading-3",
          "size": "1.5rem",
          "fluid": {
            "min": "1.5rem",
            "max": "2.25rem"
          }
        },
        {
          "name": "Heading 4",
          "slug": "heading-4",
          "size": "1.25rem",
          "fluid": {
            "min": "2rem",
            "max": "2rem"
          }
        },
        {
          "name": "Heading 5",
          "slug": "heading-5",
          "size": "1rem",
          "fluid": {
            "min": "1rem",
            "max": "1.5rem"
          }
        },
        {
          "name": "Heading 6",
          "slug": "heading-6",
          "size": "0.675rem",
          "fluid": {
            "min": "0.675rem",
            "max": "0.75rem"
          }
        }
      ],
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Noto Sans",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/noto-sans-v39-latin-regular.woff2"
              ]
            },
            {
              "fontFamily": "Noto Sans",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "400",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/noto-sans-v39-latin-italic.woff2"
              ]
            },
            {
              "fontFamily": "Noto Sans",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "600",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/noto-sans-v39-latin-600.woff2"
              ]
            },
            {
              "fontFamily": "Noto Sans",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "600",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/noto-sans-v39-latin-600italic.woff2"
              ]
            }
          ],
          "fontFamily": "\"Noto Sans\", sans-serif",
          "name": "Noto Sans",
          "slug": "noto-sans"
        },
        {
          "name": "System",
          "slug": "system",
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
        },
        {
          "name": "Serif",
          "slug": "serif",
          "fontFamily": "\"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif"
        },
        {
          "name": "Monospace",
          "slug": "monospace",
          "fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
        }
      ]
    },
    "spacing": {
      "blockGap": true,
      "padding": true,
      "margin": true,
      "units": [
        "rem",
        "px",
        "%"
      ],
      "spacingScale": {
        "steps": 12
      },
      "spacingSizes": [
        {
          "size": "0.25rem",
          "slug": "1",
          "name": "1 (4px)"
        },
        {
          "size": "0.5rem",
          "slug": "2",
          "name": "2 (8px)"
        },
        {
          "size": "0.75rem",
          "slug": "3",
          "name": "3 (12px)"
        },
        {
          "size": "1rem",
          "slug": "4",
          "name": "4 (16px)"
        },
        {
          "size": "1.5rem",
          "slug": "5",
          "name": "5 (24px)"
        },
        {
          "size": "2rem",
          "slug": "6",
          "name": "6 (32px)"
        },
        {
          "size": "2.5rem",
          "slug": "7",
          "name": "7 (40px)"
        },
        {
          "size": "4rem",
          "slug": "8",
          "name": "8 (64px)"
        },
        {
          "size": "5rem",
          "slug": "9",
          "name": "9 (80px)"
        },
        {
          "size": "8.25rem",
          "slug": "10",
          "name": "10 (132px)"
        },
        {
          "size": "12.5rem",
          "slug": "11",
          "name": "11 (264px)"
        },
        {
          "size": "20rem",
          "slug": "12",
          "name": "12 (330px)"
        }
      ]
    },
    "shadow": {
      "defaultPresets": false,
      "presets": []
    },
    "border": {
      "radius": true,
      "color": true,
      "style": true,
      "width": true
    },
    "custom": {
      "colors": {
        "primary-50": "#F1F3FF",
        "primary-100": "#CED5FD",
        "primary-200": "#7286F8",
        "primary-300": "#2F4BF4",
        "primary-400": "#001489",
        "primary-500": "#000D57",
        "primary-600": "#000524",
        "secondary-100": "#EFF7E3",
        "secondary-200": "#D3E9B4",
        "secondary-300": "#B7DB85",
        "secondary-400": "#8DC63F",
        "secondary-500": "#71A130",
        "secondary-600": "#567A24",
        "tertiary-100": "#FFF0C7",
        "tertiary-200": "#FFDF8A",
        "tertiary-300": "#FFC425",
        "tertiary-400": "#F68A33",
        "tertiary-500": "#E16A0A",
        "tertiary-600": "#A64E07",
        "neutral-0": "#FFFFFF",
        "neutral-100": "#F0F0F0",
        "neutral-200": "#CCC",
        "neutral-300": "#999",
        "neutral-400": "#6E6E6E",
        "neutral-500": "#474747",
        "neutral-600": "#242424",
        "neutral-700": "#000000",
        "white": "var(--wp--custom--colors--neutral-0)",
        "neutral": "var(--wp--custom--colors--neutral-100)",
        "dark": "var(--wp--custom--colors--neutral-600)",
        "black": "var(--wp--custom--colors--neutral-700)",
        "navy": "var(--wp--custom--colors--primary-400)",
        "green": "var(--wp--custom--colors--secondary-400)"
      },
      "border": {
        "radius": {
          "small": "var(--wp--preset--spacing--1)",
          "medium": "var(--wp--preset--spacing--2)",
          "large": "var(--wp--preset--spacing--3)"
        },
        "width": {
          "small": "1px",
          "medium": "3px"
        },
        "style": {
          "solid": "solid"
        }
      },
      "align": {
        "full": "100%"
      },
      "transitions": {
        "default": "all 0.25s ease-in-out",
        "single-property": "0.25s ease-in-out"
      },
      "cards": {
        "gap": "var(--wp--preset--spacing--6)"
      }
    },
    "blocks": {
      "core/heading": {
        "spacing": {
          "padding": false,
          "margin": false
        }
      },
      "core/paragraph": {
        "color": {},
        "spacing": {}
      },
      "core/button": {
        "color": {
          "palette": [
            {
              "slug": "navy",
              "name": "Navy",
              "color": "var(--wp--custom--colors--navy)"
            },
            {
              "slug": "green",
              "name": "Green",
              "color": "var(--wp--custom--colors--green)"
            }
          ]
        },
        "typography": {
          "fontSizes": true
        },
        "spacing": {
          "padding": false,
          "margin": false
        },
        "border": {
          "radius": false,
          "color": false,
          "style": false,
          "width": false
        }
      },
      "core/gallery": {},
      "core/table": {
        "color": {
          "text": true,
          "background": true
        }
      },
      "core/image": {
        "border": {
          "radius": false,
          "color": false,
          "style": false,
          "width": false
        }
      },
      "core/spacer": {},
      "riapress/cards": {}
    }
  },
  "styles": {
    "color": {},
    "typography": {
      "fontSize": "var(--wp--preset--font-size--medium)",
      "fontFamily": "var(--wp--preset--font-family--noto-sans)",
      "lineHeight": "1.5"
    },
    "spacing": {
      "padding": {
        "top": "var(--wp--preset--spacing--6)",
        "right": "var(--wp--preset--spacing--6)",
        "bottom": "var(--wp--preset--spacing--6)",
        "left": "var(--wp--preset--spacing--6)"
      },
      "blockGap": "var(--wp--preset--spacing--6)"
    },
    "elements": {
      "h1": {
        "typography": {
          "lineHeight": "1.3",
          "fontSize": "clamp(2.5rem, 2.5rem + ((1vw - 0.2rem) * 1.633), 3.5rem)",
          "fontWeight": "600"
        }
      },
      "h2": {
        "typography": {
          "lineHeight": "1.3",
          "fontSize": "clamp(2rem, 2rem + ((1vw - 0.2rem) * 1.633), 3rem)",
          "fontWeight": "600"
        }
      },
      "h3": {
        "typography": {
          "lineHeight": "1.4",
          "fontSize": "clamp(1.5rem, 1.5rem + ((1vw - 0.2rem) * 1.224), 2.25rem)",
          "fontWeight": "600"
        }
      },
      "h4": {
        "typography": {
          "lineHeight": "1.4",
          "fontSize": "clamp(2rem, 2rem + ((1vw - 0.2rem) * 1), 2rem)",
          "fontWeight": "600"
        }
      },
      "h5": {
        "typography": {
          "lineHeight": "1.4",
          "fontSize": "clamp(1rem, 1rem + ((1vw - 0.2rem) * 0.816), 1.5rem)",
          "fontWeight": "600"
        }
      },
      "h6": {
        "typography": {
          "lineHeight": "1.4",
          "fontSize": "clamp(0.675rem, 0.675rem + ((1vw - 0.2rem) * 0.122), 0.75rem)",
          "fontWeight": "600"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--custom--colors--navy)"
        },
        ":hover": {
          "typography": {
            "textDecoration": "underline"
          }
        },
        ":focus": {
          "typography": {
            "textDecoration": "underline"
          }
        },
        ":active": {
          "typography": {
            "textDecoration": "underline"
          }
        },
        "typography": {
          "textDecoration": "none"
        }
      },
      "button": {
        "color": {
          "text": "var(--wp--custom--colors--white)",
          "background": "var(--wp--custom--colors--navy)"
        },
        ":focus": {
          "color": {
            "text": "var(--wp--custom--colors--white)",
            "background": "var(--wp--custom--colors--primary-300)"
          }
        },
        ":hover": {
          "color": {
            "text": "var(--wp--custom--colors--white)",
            "background": "var(--wp--custom--colors--primary-300)"
          }
        }
      }
    },
    "blocks": {
      "core/button": {
        "color": {}
      }
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Site Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Site Footer"
    }
  ],
  "customTemplates": [
    {
      "name": "page-no-title",
      "postTypes": [
        "page"
      ],
      "title": "Page No Title"
    }
  ],
  "patterns": []
}