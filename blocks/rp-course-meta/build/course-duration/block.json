{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "zp/course-duration",
  "version": "0.1.0",
  "title": "Course Duration",
  "category": "text",
  "icon": "clock",
  "supports": {
    "html": false,
    "multiple": false,
    "color": {
      "background": true,
      "text": true,
      "gradients": false
    },
    "typography": {
      "fontSize": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": true,
        "padding": true
      }
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    }
  },
  "attributes": {
    "label": {
      "type": "string",
      "default": "Duration:"
    },
    "showLabel": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "course-meta",
  "editorScript": "file:index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "usesContext": [
    "postId",
    "postType"
  ],
  "render": "file:./render.php"
}