{
	"name": "rp-social-share",
	"version": "0.1.0",
	"description": "Social Share tool",
	"author": "Tomas Llobet-Arany",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build ./src/index.js",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start ./src/index.js",
		"make-pot": "wp i18n make-pot . languages/social-share.pot --slug=social-share --domain=social-share --exclude=node_modules,src,vendor",
		"make-json": "wp i18n make-json languages/ --no-purge",
		"update-po": "wp i18n update-po languages/"
	},
	"devDependencies": {
		"@wordpress/scripts": "^28.6.0"
	},
	"dependencies": {
		"@wordpress/components": "^27.0.0",
		"@wordpress/data": "^10.1.0",
		"@wordpress/element": "^6.1.0",
		"@wordpress/icons": "^10.26.0"
	}
}